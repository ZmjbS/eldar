{
	"name": "Python",
	"type": "python",
	"pythonPath":"${config.python.pythonPath}", 
	"request": "launch",
	"stopOnEntry": true,
	"console": "none",
	"program": "${workspaceRoot}/manage.py runserver",
	"cwd": "${workspaceRoot}",
	"debugOptions": [
		"WaitOnAbnormalExit",
		"WaitOnNormalExit",
		"RedirectOutput"
	],
	"env": {"name":"value"}
}